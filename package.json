    },
    "dependencies": {
       "@directus/sdk": "^19.1.0",
		"dotenv": "^16.5.0",
		"express": "^5.1.0",
		"phaser3-rex-plugins": "^1.80.16",
		"socket.io": "^4.8.1",
        "jsonwebtoken": "^9.0.2",
        "phaser": "^3.70.0"
    },
    "devDependencies": {
        "@babel/core": "^7.10.5",
		"@babel/plugin-proposal-class-properties": "^7.10.4",
		"@parcel/packager-raw-url": "^2.15.1",
		"@parcel/transformer-webmanifest": "^2.15.1",
		"autoprefixer": "^9.8.8",
		"babel-eslint": "^10.1.0",
		"buffer": "^6.0.3",
		"crypto-browserify": "^3.12.1",
		"cssnano": "4.1.11",
		"eslint": "^6.8.0",
		"minimist": ">=1.2.2",
