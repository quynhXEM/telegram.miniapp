{
  "version": 2,
  "builds": [
    { "src": "index.html", "use": "@vercel/static" },
    { "src": "index.js", "use": "@vercel/static" },
    { "src": "*.js", "use": "@vercel/node" }
  ],
  "routes": [
    { "src": "/api/auth", "dest": "/auth.js" },
    { "src": "/api/claim", "dest": "/claim.js" },
    { "src": "/(.*)", "dest": "/index.html" }
  ]
}
